<view class="exerciseDetail">
  <view class="img">
    <image src="{{result[0].img}}" mode="" />
  </view>
  <view class="currentStep">{{currentStep}}</view>
  <view class="content">
    <block wx:for="{{content}}" wx:key="index">
      <view class="item">
        {{item}}
      </view>
    </block>
  </view>

</view>